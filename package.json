{
  "name": "@kobbleio/admin",
  "version": "0.0.1",
  "description": "Official SDK to perform privileged operations on your Kobble app.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s clean build-only",
    "clean": "rimraf dist",
    "build-only": "tsc --outDir dist",
    "build-interop": "npm run build && npx denoify",
    "prepare": "husky",
    "lint": "eslint --fix src/**/*.ts",
    "npm-release": "semantic-release",
    "buildpack": "npm run build && npm pack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-angular": "^19.1.0",
    "@semantic-release/npm": "^12.0.0",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.5",
    "semantic-release-export-data": "^1.0.1",
    "typescript": "^5.3.3"
  },
  "release": {
    "registryUrl": "https://registry.npmjs.org/",
    "branches": [
      "main"
    ],
    "plugins": [
      "semantic-release-export-data",
      "@semantic-release/npm"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "jose": "^5.2.4"
  }
}
